{
  "name": "wdp301-assistants",
  "version": "1.0.0",
  "description": "A comprehensive web-based system demonstrating React, React Native, Node.js, Express, and MongoDB technologies",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "mobile": "cd mobile && npm start",
    "install-all": "npm install && cd client && npm install && cd ../mobile && npm install",
    "build": "cd client && npm run build",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "react",
    "react-native",
    "nodejs",
    "express",
    "mongodb",
    "fullstack"
  ],
  "author": "WDP301 Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "eslint": "^8.47.0",
    "eslint-config-node": "^4.1.0"
  }
}